{
  "private": true,
  "version": "1.2.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "start": "node .output/server/index.mjs",
    "deploy": "yarn build && yarn start",
    "pm2": "pm2 delete fe && pm2 start npm  --name 'fe' -- run deploy",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "staging:clean": "git fetch origin --prune 'refs/tags/*:refs/tags/*' && git push origin --delete $(git tag -l 'staging-*') || true && git fetch origin --prune 'refs/tags/*:refs/tags/*'",
    "staging:deploy": "yarn staging:clean && TAG=`echo staging-$(date +%Y%m%d-%H%M%S)` && git tag $TAG && git push origin $TAG"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@mdi/font": "^7.1.96",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "node-sass": "^8.0.0",
    "nuxt": "3.3.1",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.4",
    "vite-plugin-vuetify": "^1.0.1",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css}": [
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.6",
    "@vuepic/vue-datepicker": "^7.4.1",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^2.29.3",
    "jsonwebtoken": "^9.0.2",
    "ky": "^0.33.2",
    "ky-universal": "^0.11.0",
    "linkify-html": "^4.1.0",
    "linkifyjs": "^4.1.0",
    "pinia": "^2.0.29",
    "remove-accents": "^0.5.0",
    "sanitize.css": "^13.0.0",
    "vuetify": "^3.2.5"
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.22.17"
  }
}
